{
  "name": "seq",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env DEBUG=express-sequelize nodemon ./bin/www",
    "dev": "cross-env NO_AUTH=true npm start",
    "prod": "cross-env NODE_ENV=production node ./bin/www",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js",
    "db:migrate": "node_modules/.bin/sequelize db:migrate"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.1.0",
    "keycloak-connect": "^11.0.1",
    "lusca": "^1.6.1",
    "morgan": "^1.10.0",
    "pg": "^8.3.2",
    "pug": "^3.0.0",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "expect.js": "^0.3.1",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  }
}
